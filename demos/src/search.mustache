<header class='o-header-services o-header-services__primary-nav--search' data-o-component='o-header'>
	<div class='o-header-services__top o-header-services__container'>
		<div class='o-header-services__hamburger'>
			<a class='o-header-services__hamburger-icon' href="#o-header-drawer"	aria-controls="o-header-drawer"><span class="o-header__visually-hidden">Menu</span></a>
		</div>
		<div class='o-header-services__ftlogo'></div>
		<div class='o-header-services__title'>
			<h1 class='o-header-services__product-name'><a href="/">Tool or Service name</a></h1><span class='o-header-subrand__product-tagline'>Tagline to explain the product here</span>
		</div>
		<div class='o-header-services__related-content'>
			<a href="#o-header-search" class="o-header__top-link o-header__top-link--search" aria-controls="o-header-search-js" title="Open search bar">
				<span class="o-header__top-link-label">Search</span>
			</a>
		</div>
	</div>

	<div class="o-header-services__search">
		<div id="o-header-search-js" class="o-header__row o-header__search o-header-services__container" role="search" data-o-header-search>
			<div class="o-header__container ">
				<form class="o-forms" action="/search" role="search" aria-label="Site search">
					<div class="o-forms__affix-wrapper">

						<label class="o-header__visually-hidden" for="o-header-search-term-js">Search the ??</label>
						<input class="o-forms__text" id="o-header-search-term-js" name="q" type="text" placeholder="Do the searching thing">
						<button class="o-header-services__search-button" type="submit">
							Search
						</button>
						<button class="o-header__search-close o--if-js" type="button" aria-controls="o-header-search-js" title="Close search bar">
							<span class="o-header__visually-hidden">Close</span>
						</button>
					</div>
				</form>
			</div>
		</div>
	</div>

	<nav class='o-header-services__primary-nav'>
		<div class='o-header-services__container'>
			<ul class='o-header-services__nav-list'>
				<li class='o-header-services__nav-item'>
